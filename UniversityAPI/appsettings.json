{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Jwt": {
    "Key": "",
    "Issuer": "YourAppName",
    "Audience": "YourAppClient"
  },
  "Localization": {
    "DefaultCulture": "en",
    "SupportedCultures": [ "en", "ja", "fr" ]
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 60
      }
    ]
  },
  "Cors": {
    "AllowedOrigins": [ "https://your-frontend.com" ]
  },
  "default_standard_time": "Singapore Standard Time"
}
